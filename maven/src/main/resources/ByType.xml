<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.0.xsd">


	<bean id="employee"
		class="com.bridgeit.AutowireByType.EmployeeBean" autowire="byType">
		<property name="name" value="Bridgelabz" />
	</bean>

	<bean id="departmentBean"
		class="com.bridgeit.AutowireByType.DepartmentBean">
		<property name="book" value="Product Engineer" />
	</bean>

</beans>